{
    "manifest_version": 2,
    "name": "Nodeachrome",
    "version": "0.0.1",
    "description": "Nodeachrome",
    "app": {
        "background": {
            "scripts": [ "background.js" ]
        }
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "http://*/",
        "https://*/",
        "nativeMessaging"
    ]
}
